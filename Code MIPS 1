1.	#Calcul du pgcd de i et j, avec i = 108 et j = 36
2.	li $t0, 108
3.	li $t1, 36

4.	while :	beq $t0, $t1, endwhile 		# saut de 5

5.	if    :	ble $t0, $t1, else			# saut de 2

6.	then  :	sub $t0, $t0, $t1			# i = i - j
7.			b endif						# saut de 1

8. 	else  : sub $t1, $t1, $t0			# j = j - i

9. 	endif : b while						# saut de -6			

10.	endwhile : halt