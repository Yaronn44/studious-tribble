# Calcul de la suite de Fibonacci pour un n donné
li $t0, 0						# a = 0
li $t1, 1 						# b = 1
li $t2, 0 						# c = 0
li $t3, 10						# n = 10
li $t4, 1 						# i = 1 
li $t5, 0						# valeur pour move 
li $t6, 1 						# valeur pour incrémentation i

for	:	bge $t4, $t3, endfor 	# saut de 5
		add $t2, $t1, $t5		# c = b
		add $t1, $t1, $t0		# b = a + b
		add $t0, $t2, $t5		# a = c
		add $t4, $t4, $t6		# ++i
		b for					# saut de -6
endfor :
halt